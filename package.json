{
  "name": "react-ssr-app",
  "version": "1.0.0",
  "description": "React SSR Template App with Vike",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": ">=8.5.0",
    "yarn": ">=1.22.18",
    "node": ">=16.14.2"
  },
  "type": "module",
  "homepage": "https://github.com/epifanovmd/react-ssr",
  "author": {
    "name": "Epifanov Andrei",
    "email": "epifanovmd@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epifanovmd/react-ssr.git"
  },
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build",
    "server": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production node --no-warnings=ExperimentalWarning --loader ts-node/esm ./server/index.ts",
    "preview": "vite build && vite preview",
    "lint:fix": "eslint --fix \"**/*{.ts,.tsx}\"",
    "prettier:fix": "prettier --parser typescript --write src/**/*.{json,ts,tsx}",
    "check-packages-updates": "yarn outdated"
  },
  "dependencies": {
    "@force-dev/react": "^1.1.2",
    "@force-dev/react-front": "^1.0.13",
    "@force-dev/utils": "^1.1.60",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "express": "^4.18.1",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "lodash": "^4.17.21",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "query-string": "^6.14.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^14.1.2",
    "reflect-metadata": "^0.1.13",
    "socket.io-client": "^4.7.5",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^3.0.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.9",
    "@types/query-string": "^6.3.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "8.22.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vike": "^0.4.177",
    "vite": "^5.3.1",
    "vite-plugin-cjs-interop": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix \"src/**/*{.ts,.tsx}\"",
      "prettier --parser typescript --write src/**/*.{ts,tsx}",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "IE 10",
    "not op_mini all"
  ]
}
